local Tab = Window:CreateTab("Troll Slapping tower", 4483362458) -- Title, Image

local Button = Tab:CreateButton({
   Name = "Get Gloves [Troll features] (auto spam no work)",
   Callback = function() loadstring(game:HttpGet("https://raw.githubusercontent.com/LUNOXLOL/Script/refs/heads/main/TrollSlappingTower"))()
   -- The function that takes place when the button is pressed
   end,
})

local Button = Tab:CreateButton({
   Name = "Auto farm Slaps (Only use with god hand)",
   Callback = function() local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local tool = LocalPlayer.Character["God's Hand"]

repeat
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local hrp = player.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                local args = {
                    [1] = "slash",
                    [2] = player.Character,
                    [3] = hrp.Position
                }
                
                tool.Event:FireServer(unpack(args))
            end
        end
    end
    
    tool.Event:FireServer("timestop")
    
    task.wait(0.0)
until false
   -- The function that takes place when the button is pressed
   end,
})

